{
  "name": "fast-replace-diacritics",
  "version": "1.0.1",
  "description": "2-5x faster diacritics replacement compared to traditional 'str.replace'",
  "keywords": [
    "diacritics",
    "replace-diacritics",
    "remove-diacritics"
  ],
  "homepage": "https://github.com/Nerivec/fast-replace-diacritics#readme",
  "bugs": {
    "url": "https://github.com/Nerivec/fast-replace-diacritics/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nerivec/fast-replace-diacritics.git"
  },
  "license": "GPL-3.0-or-later",
  "author": "Nerivec",
  "files": [
    "./dist"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --config ./test/vitest.config.mts",
    "test:cov": "vitest run --config ./test/vitest.config.mts --coverage",
    "bench": "vitest bench --run --config ./test/vitest.config.mts",
    "check": "biome check --write .",
    "check:ci": "biome check .",
    "prepack": "npm run build"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@vitest/coverage-v8": "^4.0.16",
    "tsdown": "^0.18.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.12"
  }
}